{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/[lng]/login/Login_segment/Login_segment.jsx",
    "app/[lng]/login/Login_segment/Login_component/Login_component.jsx",
    "app/member_setting/layout.tsx",
    "app/[lng]/project_setting/content_management_segment/projectlistblock_component/projectlistblock_component.jsx",
    "app/[lng]/project_setting/content_management_segment/selectfaucetgroup_component/selectfaucetgroup_component.jsx",
    "app/[lng]/project_setting/content_management_segment/Top_label_component/Top_label_component.jsx",
    "app/[lng]/component/Navbar.jsx",
    "app/[lng]/faucet_ctrl/faucetitem_segment/faucetlistblock_component/faucetlistblock_component.tsx",
    "app/[lng]/component/LoadingScreen.jsx",
    "app/[lng]/component/BackButton.jsx",
    "app/[lng]/component/Title.tsx",
    "app/[lng]/account_setting/layout.tsx",
    "app/[lng]/faucet_ctrl/page.jsx",
    "app/[lng]/project_setting/project_button_segment/Project_managerment_component/Project_managerment_component.jsx",
    "app/[lng]/faucet_ctrl/faucetitem_segment/faucetitem_segment.jsx",
    "app/[lng]/faucet_ctrl/faucetitem_segment/faucetlistblock_component/service/faucetlistblock_hooks.jsx",
    "app/[lng]/faucet_ctrl/faucet_control_segment/service/faucet_control_segment_hook.jsx",
    "app/[lng]/faucet_ctrl/faucetitem_segment/faucetinfo_component/faucetinfo_component.jsx",
    "app/[lng]/project_setting/page.jsx",
    "app/[lng]/project_setting/project_button_segment/Project_button_segment.jsx",
    "app/[lng]/faucet_usage/[...faucet_uid]/page.jsx",
    "app/[lng]/faucet_usage/page.jsx",
    "app/[lng]/faucet_usage/Usage_segment/Usage_segment.jsx",
    "app/[lng]/faucet_usage/Usage_segment/Title_component/Title_component.jsx",
    "app/[lng]/faucet_usage/Usage_segment/FaucetID_component/FaucetID_component.jsx",
    "app/[lng]/faucet_usage/Usage_segment/UsageGroup_component/UsageGroup_component.jsx",
    "app/[lng]/faucet_usage/Usage_segment/UsageGroup_component/service/UsageGroup_hooks.jsx",
    "app/[lng]/faucet_usage/Figure_segment/Figure_segment.jsx",
    "app/[lng]/faucet_usage/Figure_segment/Timeoption_component/Timeoption_component.jsx",
    "app/[lng]/faucet_usage/Figure_segment/Timeoption_component/service/Timeoption_hooks.jsx",
    "app/[lng]/faucet_usage/Figure_segment/Figure_component/Figure_component.jsx",
    "app/[lng]/faucet_usage/Figure_segment/Figure_component/service/Figure_hooks.jsx"
  ],
  "exclude": ["node_modules"]
}
